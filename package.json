  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "@testing-library/jest-native": "^3.4.3",
    "@testing-library/react-native": "^7.1.0",
    "@types/babel__core": "^7.1.12",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.14.21",
    "@types/react-native": "^0.63.2",
